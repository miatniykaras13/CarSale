{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  
  "AllowedHosts": "*",
  
  "AdExpiration": {
    "AdLifeSpan": "744:00:00",
    "CheckInterval": "00:5:00"
  },
  
  "Keycloak": {
    "AuthorizationUrl": "http://localhost:8080/realms/carsale/protocol/openid-connect/auth",
    "TokenUrl": "http://localhost:8080/realms/carsale/protocol/openid-connect/token"
  },
  
  "Authentication": {
    "MetadataAddress": "http://localhost:8080/realms/carsale/.well-known/openid-configuration",
    "ValidIssuer": "http://localhost:8080/realms/carsale",
    "Audience": "adservice"
  },
  
  "FileStorage": {
    "Endpoint": "https://localhost:6061",
    "MaxFileSize": 10485760
  },
  
  "AutoCatalog": {
    "Endpoint": "https://localhost:6060"
  },
  
  "Cache": {
    "MemoryAbsoluteExpiration": "00:1:00",
    "DistributedAbsoluteExpiration": "00:10:00"
  },
  
  "Kestrel": {
    "EndpointsDefaults": {
      "Protocols": "Http2"
    }
  }
}
